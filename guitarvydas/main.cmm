#include "mpos.h"
#include "producer.h"
#include "consumer.h"
#include "kernel.h"

int main (int argc, char **argv) {
  Component p = $newComponent (initProducer, reactProducer);
  Component c = $newComponent (initConsumer, reactConsumer);
  ListCell lis1 = $newListCellComponent (p);
  ListCell lis2 = $newListCellComponent (c);
  $append (lis1, lis2);
  runQueue = lis1;
  $connect (p, c);
  Dispatcher ();
  return 0;
}
